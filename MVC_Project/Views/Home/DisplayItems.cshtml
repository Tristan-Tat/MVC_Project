<h1>DISPLAY ITEMS</h1>
<br/>

<ul>
    @{
        List<ItemModel> items = (List<ItemModel>) ViewData["List"];

        foreach (ItemModel item in items)
        {
            <li>
            @item.ToString()
            </li>

        }

    }
</ul>
